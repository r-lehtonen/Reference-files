@CONFERENCE{Nakonechnyi2020209,
author={Nakonechnyi, A. and Martsenyuk, V. and Sverstiuk, A. and Arkhypova, V. and Dzyadevych, S.},
title={Investigation of the mathematical model of the biosensor for the measurement of α-chaconine based on the impulsive differential system},
journal={CEUR Workshop Proceedings},
