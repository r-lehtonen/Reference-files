title={Chemical constituents, antioxidant, anticholinesterase and antiproliferative effects of Algerian pistacia atlantica desf. Extracts},
journal={Recent Patents on Food, Nutrition and Agriculture},
year={2020},
volume={11},
number={3},
pages={249-256},
doi={10.2174/2212798411666200207101502},
